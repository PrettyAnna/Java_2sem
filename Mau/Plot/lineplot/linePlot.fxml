<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>

<VBox xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.lineplot.LinePlot">
        <Button mnemonicParsing="false" onAction="#openDataLine" prefHeight="61.0" prefWidth="500.0" text="Выбор данных для графика"
                textAlignment="CENTER">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <LineChart fx:id="linePlot">
            <xAxis>
                <NumberAxis side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" />
            </yAxis>
        </LineChart>
    </VBox>
